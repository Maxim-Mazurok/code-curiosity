.row
  %section.content
    .col-md-2
      = render 'sidebar_filters'
    .col-md-10
      .box.box-primary
        .box-header.with-border
          %h3.box-title Commits
          .box-tools.text-center.commits-tools#commits_pagination
            = paginate @commits, remote: true, window: 1, outer_window: 1
        .box-body#commits
          = render partial: 'judging/commit', collection: @commits
:javascript
  showRatingControl();
